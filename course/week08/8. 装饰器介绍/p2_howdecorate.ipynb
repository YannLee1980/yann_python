   "outputs": [
    {
     "data": {
      "text/plain": [
       "'<html><body>Hello World.</body></html>'"
      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def html(func):\n",
    "    def decorator():\n",
    "        return f'<html>{func()}</html>'\n",
    "    return decorator\n",
    "def body(func):\n",
    "    def decorator():\n",
    "        return f'<body>{func()}</body>'\n",
    "    return decorator\n",
    "@html\n",
    "@body\n",
    "def content():\n",
    "    return 'Hello World.'\n",
    "\n",
    "content()"
   ]
